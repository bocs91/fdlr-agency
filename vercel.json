{
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/src/assets/icons/**/*.svg",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/svg+xml"
          }
        ]
      },
      {
        "source": "/src/assets/images/**/*.jpg",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/jpeg"
          }
        ]
      },
      {
        "source": "/src/assets/images/**/*.png",
        "headers": [
          {
            "key": "Content-Type",
            "value": "image/png"
          }
        ]
      }
    ]
  }
  